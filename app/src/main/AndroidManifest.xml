<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ColossusMachina.realitycheck">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_a"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_a"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ColossusMachina.realitycheck.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ColossusMachina.realitycheck.TipsActivity" />
        <receiver android:name="com.ColossusMachina.realitycheck.NotificationPublisher" />
    </application>

</manifest>